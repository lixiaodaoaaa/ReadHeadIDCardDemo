<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/left_rb_bg" android:state_checked="true"></item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="#F8F8F8"></solid>
            <corners android:bottomLeftRadius="10dp" android:topLeftRadius="10dp"></corners>
            <stroke android:width="1dp" android:color="#AAAAAA"></stroke>
        </shape>
    </item>
</selector>