<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/right_rb_bg" android:state_checked="false"></item>
    <item android:state_checked="true">
        <shape>
            <solid android:color="#2FA0FF"></solid>
            <corners android:bottomRightRadius="10dp" android:topRightRadius="10dp"></corners>
            <stroke android:width="1dp" android:color="#AAAAAA"></stroke>
        </shape>
    </item>
</selector>